{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["**/range-slider/*"],
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "max-len": ["error", { "code": 80 }],
    "linebreak-style": ["error", "windows"]
  }
}